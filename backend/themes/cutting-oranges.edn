                           :font        "Cabinet Grotesk"
                           :labelColor  "#ffffff"
                           :titleFont   "Cabinet Grotesk"
                           :titleColor  "#ffffff"
                           :labelFont   "Cabinet Grotesk"
                           :domain      false
                           :gridColor   "#ffffff11"
                           :tickColor   "#FF6B4A44"
