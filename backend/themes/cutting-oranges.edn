                           :labelFont    "Cabinet Grotesk"
                           :titleFont    "Cabinet Grotesk"
                           :titleColor   "#ffffff"}
                 :point   {:font "Cabinet Grotesk" :stroke "#FF6B4A" :color "#FF6B4A65"}
                 :area    {:font "Cabinet Grotesk" :stroke "#FF6B4A" :color "#FF6B4A65"}
                 :axis    {:domainColor "#FF6B4A44"
