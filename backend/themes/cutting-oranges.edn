   {:box-shadow
      "inset 0 1px 0 rgba(255,255,255,0.05),                                      0 8px 40px rgba(0,0,0,0.3),                                      0 2px 4px rgba(0,0,0,0.2)"
    :transform "perspective(2000px)"
    :transition "all 0.4s cubic-bezier(0.23, 1, 0.32, 1)"
    :background
      "linear-gradient(135deg,                                                      rgba(255, 107, 74, 0.03) 0%,                                                      rgba(17, 17, 24, 0.6) 100%)"
    :backdrop-filter "blur(20px) brightness(105%)"
    :filter "none"
    :text-shadow "none"
    :letter-spacing "0.01em"
    :border "2px solid rgba(255, 107, 74, 0.1)"
    :border-radius "24px 4px 16px 4px"}
 :vega-defaults {:mark    {:font "Cabinet Grotesk" :stroke "#FF6B4A" :color "#FF6B4A65"}
                 :legend  {:labelFont  "Cabinet Grotesk"
                           :legendFont "Cabinet Grotesk"
                           :labelColor "#ffffff"
                           :titleColor "#ffffff"
                           :stroke     "#FF6B4A"
                           :titleFont  "Cabinet Grotesk"}
                 :header  {:labelFont "Cabinet Grotesk" :titleFont "Cabinet Grotesk" :titleColor "#ffffff"}
