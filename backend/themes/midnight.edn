 :base-block-style
   {:transform-style "preserve-3d"
    :box-shadow
      "inset 0 0 30px rgba(0,0,0,0.5),                                      inset 0 2px 1px rgba(255,255,255,0.05),                                      0 15px 35px -20px rgba(0,0,0,0.8),                                      0 0 20px rgba(55, 48, 163, 0.1),                                      0 0 0 1px rgba(55, 48, 163, 0.2)"
    :transform "perspective(2000px) rotateX(1deg) translateZ(5px)"
    :transition "all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1)"
    :background
      "linear-gradient(135deg,                                                      rgba(0,0,0,0.7) 0%,                                                      rgba(55,48,163,0.05) 50%,                                                     rgba(0,0,0,0.7) 100%),                                      radial-gradient(circle at 50% 0%,                                                    rgba(55,48,163,0.15) 0%,                                                    transparent 70%)"
    :background-blend-mode "overlay, screen"
    :backdrop-filter "blur(20px) brightness(90%) contrast(120%)"
    :filter "none"
    :text-shadow "0 0 30px rgba(55, 48, 163, 0.2)"
    :border "1px solid rgba(255, 255, 255, 0.05)"
    :border-radius "6px"}
