                              :fontSize        "13px"
                              :border          "1px solid rgba(79, 70, 229, 0.2)"
                              :text            {:fill "#ffffff"}}
                  :basic     {:whiteSpace "pre" :display "flex" :alignItems "center"}
                  :tableCell {:padding "6px 10px"}}
