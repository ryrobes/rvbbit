{:codemirror-theme "rvbbit-dynamic"
 :editor-param-background-color "#080318"
 :pop-2 "#4f46e5"
 :base-block-style
   {:transform-style "preserve-3d"
    :box-shadow
      "inset 0 0 50px rgba(255,255,255,0.05),                                      inset 0 2px 1px rgba(255,255,255,0.1),                                      0 20px 40px -20px rgba(0,0,0,0.5),                                      0 0 40px rgba(79, 70, 229, 0.1),                                      0 0 0 1px rgba(79, 70, 229, 0.1)"
    :transform "perspective(2000px) rotateX(1deg) translateZ(10px)"
    :transition "all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1)"
    :background
      "linear-gradient(135deg,                                                      rgba(255,255,255,0.03) 0%,                                                      rgba(79,70,229,0.01) 50%,                                                     rgba(0,0,0,0.05) 100%),                                      radial-gradient(circle at 50% 0%,                                                    rgba(79,70,229,0.15) 0%,                                                    transparent 70%)"
    :background-blend-mode "overlay, screen"
    :backdrop-filter "blur(30px) saturate(180%)"
    :filter "none"
    :text-shadow "0 0 30px rgba(79, 70, 229, 0.2)"
    :border "1px solid rgba(255, 255, 255, 0.1)"
    :border-radius "16px"}
 :vega-defaults {:mark    {:font "Plus Jakarta Sans" :stroke "#4f46e5" :color "#4f46e565"}
                 :legend  {:labelFont  "Plus Jakarta Sans"
