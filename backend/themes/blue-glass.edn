                           :titleColor "#ffffff"
                           :stroke     "#4f46e5"
                           :titleFont  "Plus Jakarta Sans"}
                 :header  {:labelFont "Plus Jakarta Sans" :titleFont "Plus Jakarta Sans" :titleColor "#ffffff"}
                 :title   {:font         "Plus Jakarta Sans"
                           :subtitleFont "Plus Jakarta Sans"
