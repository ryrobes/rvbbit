                           :titleFont    "Plus Jakarta Sans"
                           :titleColor   "#ffffff"}
                 :point   {:font "Plus Jakarta Sans" :stroke "#4f46e5" :color "#4f46e565"}
                 :area    {:font "Plus Jakarta Sans" :stroke "#4f46e5" :color "#4f46e565"}
                 :axis    {:domainColor "#4f46e544"
                           :grid        true
                           :font        "Plus Jakarta Sans"
                           :labelColor  "#ffffff"
                           :titleFont   "Plus Jakarta Sans"
                           :titleColor  "#ffffff"
                           :labelFont   "Plus Jakarta Sans"
                           :domain      false
                           :gridColor   "#ffffff11"
                           :tickColor   "#4f46e544"
                           :axisFont    "Plus Jakarta Sans"}
                 :tooltip {:fill "#080318ee" :stroke "#4f46e5" :strokeWidth 1 :color "#ffffff"}
                 :view    {:stroke "transparent"}}
