               "boolean"     "#c0c0ff"
               "map"         "#8080ff"
               "list"        "#d0d0ff"
               "string"      "#a0a0ff"
               "any"         "#a0a0ff"
               "vector"      "#8080ff"
               "keyword"     "#d0d0ff"
