                    :border-radius   "0px"
                    :filter          "brightness(110%)"
                    :backdrop-filter "brightness(30%) blur(0px)"}
 :vega-defaults
   {:mark    {:font "C64 Pro" :stroke "#0000aa" :color "#0000aa65"}
    :legend  {:labelFont  "C64 Pro"
              :legendFont "C64 Pro"
              :labelColor "#a0a0ff99"
              :titleColor "#a0a0ff99"
