              :titleFont  "C64 Pro"}
    :header  {:labelFont "C64 Pro" :titleFont "C64 Pro" :titleColor "#a0a0ff99"}
    :title   {:font "C64 Pro" :subtitleFont "C64 Pro" :labelFont "C64 Pro" :titleFont "C64 Pro" :titleColor "#a0a0ff99"}
    :point   {:font "C64 Pro" :stroke "#0000aa" :color "#0000aa65"}
    :area    {:font "C64 Pro" :stroke "#0000aa" :color "#0000aa65"}
    :axis    {:domainColor "#a0a0ff22"
              :grid        true
              :font        "C64 Pro"
              :labelColor  "#a0a0ff88"
              :titleFont   "C64 Pro"
              :titleColor  "#a0a0ff99"
              :labelFont   "C64 Pro"
              :domain      false
              :gridColor   "#a0a0ff22"
              :tickColor   "#a0a0ff22"
              :axisFont    "C64 Pro"}
    :tooltip {:fill "#0000aa" :stroke "#a0a0ff" :strokeWidth 1 :color "#a0a0ff"}
    :view    {:stroke "transparent"}}
 :grid-selected-column-css {:background-color "#0000aa88" :filter "brightness(200%) contrast(120%)"}
