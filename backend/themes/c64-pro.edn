   {:font-family "C64 Pro"
    :labels      {:text {:fill "#a0a0ff" :font-size "16px" :font-family "C64 Pro" :font-weight 400}}
    :tooltip     {:container {:background   "#0000aa"
                              :color        "#a0a0ff"
                              :text         {:fill "#a0a0ff"}
                              :fontSize     "18px"
                              :borderRadius "0px"
                              :boxShadow    "0 1px 2px #a0a0ff55"
                              :padding      "5px 9px"}
                  :basic     {:whiteSpace "pre" :display "flex" :alignItems "center"}
                  :tableCell {:padding "3px 5px"}}
