{:codemirror-theme "rvbbit-dynamic"
 :editor-param-background-color "#0a0a12"
 :pop-2 "#5e72eb"
 :base-block-style
   {:box-shadow
      "inset 0 0 50px #ffffff05,                                      inset 0 0 100px #5e72eb08,                                      0 10px 30px #00000066,                                      0 -1px 0px #ffffff22,                                      0 0 0 1px #ffffff05,                                      0 20px 40px -20px #00000088"
    :transform "perspective(1000px) rotateX(1deg)"
    :transition "all 0.4s cubic-bezier(0.4, 0, 0.2, 1)"
    :background
      "linear-gradient(135deg,                                                      #0e0e1855 0%,                                                      #16162255 50%,                                                     #1e1e2c55 100%),                                      radial-gradient(circle at 50% 0%,                                                     #5e72eb11 0%,                                                     transparent 60%),                                      linear-gradient(to bottom,                                                    #ffffff03 0%,                                                    #00000022 100%)"
    :background-blend-mode "overlay, screen, normal"
    :backdrop-filter "blur(20px) saturate(180%)"
    :hover
      {:transform "perspective(1000px) rotateX(0deg) translateY(-2px)"
       :box-shadow
         "inset 0 0 50px #ffffff08,                                                 inset 0 0 100px #5e72eb11,                                                 0 15px 35px #00000077,                                                 0 -1px 0px #ffffff33,                                                 0 0 0 1px #ffffff08,                                                 0 25px 45px -20px #00000099"}
    :filter "none"
    :transform-origin "50% 0%"
    :text-shadow "0 0 30px #5e72eb22"
    :border "1px solid rgba(94, 114, 235, 0.1)"
    :border-radius "12px"}
 :vega-defaults {:mark {:font "Inter" :stroke "#5e72eb" :color "#5e72eb65"}
                 :legend {:labelFont  "Inter"
