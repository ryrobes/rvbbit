                 :header {:labelFont "Inter" :titleFont "Inter" :titleColor "#ffffff"}
                 :title
                   {:font "Inter" :subtitleFont "Inter" :labelFont "Inter" :titleFont "Inter" :titleColor "#ffffff"}
                 :point {:font "Inter" :stroke "#5e72eb" :color "#5e72eb65"}
                 :area {:font "Inter" :stroke "#5e72eb" :color "#5e72eb65"}
                 :axis {:domainColor "#5e72eb44"
                        :grid        true
                        :font        "Inter"
                        :labelColor  "#ffffff"
                        :titleFont   "Inter"
                        :titleColor  "#ffffff"
                        :labelFont   "Inter"
                        :domain      false
                        :gridColor   "#ffffff11"
                        :tickColor   "#5e72eb44"
                        :axisFont    "Inter"}
                 :tooltip {:fill "#0a0a12ee" :stroke "#5e72eb" :strokeWidth 1 :color "#ffffff"}
                 :view {:stroke "transparent"}}
